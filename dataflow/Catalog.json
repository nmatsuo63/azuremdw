{
	"name": "Catalog",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Southridge_Catalog2",
						"type": "DatasetReference"
					},
					"name": "SouthridgeCatalog"
				},
				{
					"dataset": {
						"referenceName": "VanArsdelActors",
						"type": "DatasetReference"
					},
					"name": "VanArsdelActors"
				},
				{
					"dataset": {
						"referenceName": "VanArsdelMovies",
						"type": "DatasetReference"
					},
					"name": "VanArsdelMovies"
				},
				{
					"dataset": {
						"referenceName": "VanArsdel_MovieActors",
						"type": "DatasetReference"
					},
					"name": "VanArsdelMovieActors"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "select2"
				},
				{
					"name": "select3"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "union1"
				},
				{
					"name": "VanArsdelJoin1"
				},
				{
					"name": "VanArsdelJoin2"
				},
				{
					"name": "derivedColumn2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string,",
				"          Column_8 as string,",
				"          Column_9 as string,",
				"          Column_10 as string,",
				"          Column_11 as string,",
				"          Column_12 as string,",
				"          Column_13 as string,",
				"          Column_14 as string,",
				"          Column_15 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SouthridgeCatalog",
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> VanArsdelActors",
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> VanArsdelMovies",
				"source(output(",
				"          MovieActorID as string,",
				"          MovieID as string,",
				"          ActorID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> VanArsdelMovieActors",
				"SouthridgeCatalog select(mapColumn(",
				"          Actor = Column_1,",
				"          ReleaseDate = Column_2,",
				"          Genre = Column_3,",
				"          Rating = Column_4,",
				"          AvailabilityYear = Column_5,",
				"          AvailabilityDate = Column_7,",
				"          MovieTier = Column_8,",
				"          MovieTitle = Column_9",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"VanArsdelActors select(mapColumn(",
				"          ActorID = Column_1,",
				"          ActorName = Column_2,",
				"          Gender = Column_3",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"VanArsdelMovies select(mapColumn(",
				"          MovieID = Column_1,",
				"          MovieTitle = Column_2,",
				"          Category = Column_3,",
				"          Rating = Column_4,",
				"          RuntTimeMin = Column_5,",
				"          ReleaseDate = Column_6",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"select1 derive(SourceID = 1,",
				"          ReleaseDate = toTimestamp(ReleaseDate),",
				"          AvailabilityYear = toInteger(AvailabilityYear),",
				"          AvailabilityDate = toTimestamp(AvailabilityDate),",
				"          MovieTier = toInteger(MovieTier)) ~> derivedColumn1",
				"derivedColumn1 union(byName: true)~> union1",
				"VanArsdelMovieActors, select3 join(VanArsdelMovieActors@MovieID == select3@MovieID,",
				"     joinType:'inner',",
				"     broadcast: 'auto')~> VanArsdelJoin1",
				"VanArsdelJoin1, select2 join(VanArsdelMovieActors@ActorID == select2@ActorID,",
				"     joinType:'inner',",
				"     broadcast: 'auto')~> VanArsdelJoin2",
				"VanArsdelJoin2 derive(SourceID = 2,",
				"          AvailabilityYear = toInteger(right(ReleaseDate, 4)),",
				"          AvailabilityDate = left(ReleaseDate, 5)) ~> derivedColumn2"
			]
		}
	}
}