{
	"name": "Dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText3",
						"type": "DatasetReference"
					},
					"name": "OrderDetails"
				},
				{
					"dataset": {
						"referenceName": "DelimitedText4",
						"type": "DatasetReference"
					},
					"name": "Orders"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "select2"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "derivedColumn4"
				},
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string,",
				"          Column_8 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> OrderDetails",
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Orders",
				"OrderDetails select(mapColumn(",
				"          OrderDetailsID = Column_1,",
				"          OrderID = Column_2,",
				"          MovieID = Column_3,",
				"          Quantity = Column_4,",
				"          {Unit Cost} = Column_5,",
				"          {Line Number} = Column_6",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"Orders select(mapColumn(",
				"          OrderID = Column_1,",
				"          CustomerID = Column_2,",
				"          OrderDate = Column_3,",
				"          ShipDate = Column_4,",
				"          TotalCost = Column_5",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select1 derive(SourceID = 1) ~> derivedColumn1",
				"derivedColumn1 derive(UniqueOrderID = concat(toString(SourceID), OrderID),",
				"          UniqueMovieID = concat(toString(SourceID), MovieID),",
				"          Quantity = toInteger(Quantity),",
				"          {Unit Cost} = toFloat({Unit Cost}),",
				"          {Line Number} = toInteger({Line Number})) ~> derivedColumn2",
				"select2 derive(SourceID = 1) ~> derivedColumn3",
				"derivedColumn3 derive(UniqueCustomerID = concat(toString(SourceID), CustomerID),",
				"          OrderDate = toTimestamp(OrderDate),",
				"          ShipDate = toTimestamp(ShipDate),",
				"          TotalCost = toFloat(TotalCost)) ~> derivedColumn4",
				"derivedColumn2, derivedColumn4 join(select1@OrderID == select2@OrderID,",
				"     joinType:'inner',",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}