{
	"name": "Dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SouthridgeCustomer",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "VanArsdelCustomer",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string,",
				"          Column_8 as string,",
				"          Column_9 as string,",
				"          Column_10 as string,",
				"          Column_11 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"source1 select(mapColumn(",
				"          CustomerID = Column_1,",
				"          LastName = Column_2,",
				"          FirstName = Column_3,",
				"          PhoneNumber = Column_4,",
				"          CreatedDate = Column_5,",
				"          UpdatedDate = Column_6",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive(SourceID = 1) ~> derivedColumn1",
				"derivedColumn1 derive(UniqueID = concat(toString(SourceID), CustomerID)) ~> derivedColumn2",
				"select2 derive() ~> derivedColumn3",
				"source2 select(mapColumn(",
				"          CustomerID = Column_1,",
				"          LastName = Column_2,",
				"          FirstName = Column_3,",
				"          AddressLine1 = Column_4,",
				"          AddressLine2 = Column_5,",
				"          City = Column_6,",
				"          State = Column_7,",
				"          ZipCode = Column_8,",
				"          PhoneNumber = Column_9,",
				"          CreatedDate = Column_10,",
				"          UpdatedDate = Column_11",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2"
			]
		}
	}
}